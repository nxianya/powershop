<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xianyu.mapper.ProdTagMapper">
    <resultMap id="BaseResultMap" type="com.xianyu.domain.ProdTag">
        <!-- 【表字段(不可省略)】【表字段类型】   【类属性(不可省略)】   【类属性类型】-->
        <id     column="id"  jdbcType="BIGINT"  property="id"   />
        <result column="title" jdbcType="VARCHAR" property="title" />
        <result column="status" jdbcType="BOOLEAN" property="status" />
        <result column="style"   jdbcType="INTEGER" property="style"   />
        <result column="seq"   jdbcType="INTEGER" property="seq"   />
        <result column="create_time"   jdbcType="TIMESTAMP" property="createTime"   />
        <result column="update_time"   jdbcType="TIMESTAMP" property="updateTime"   />
    </resultMap>
    <sql id="Base_Column_List">
        id,title,`status`,`style`,seq,create_time,update_time
    </sql>
</mapper>